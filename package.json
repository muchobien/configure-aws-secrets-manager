{
  "name": "@muchobien/configure-aws-secrets-manager",
  "version": "1.0.1",
  "private": true,
  "description": "A Github action to load secrets from AWS Secrets Manager into the environment",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muchobien/configure-aws-secrets-manager.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Rodrigo Sanabria <rodrigo@sanabria.dev>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.9.1",
    "@aws-sdk/client-secrets-manager": "3.165.0"
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@types/node": "18.7.16",
    "@typescript-eslint/parser": "5.36.2",
    "@vercel/ncc": "0.34.0",
    "aws-sdk-client-mock": "^1.0.0",
    "eslint": "8.23.0",
    "eslint-plugin-github": "4.3.7",
    "eslint-plugin-jest": "26.8.7",
    "jest": "28.1.3",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "typescript": "4.8.2"
  }
}
